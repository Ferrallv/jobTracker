<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<title>Contacts</title>
</head>
<body>
	<h1>Contacts</h1>
	<table>
		<tr>
			<th>Name</th>
			<th>Position</th>
			<th>Number</th>
			<th>Email</th>
			<th>Company</th>
			<th>Note</th>
		</tr>
		{{range .}}
		<tr>
			<td>{{.Name}}</td>
			<td>{{.Position}}</td>
			<td>{{.Number}}</td>
			<td>{{.Email}}</td>
			<td>{{.Company}}</td>
			<td>{{.Note}}</td>
		</tr>
		{{end}}
	</table>
</body>
</html>
